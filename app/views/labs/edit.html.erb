<div class="col-md-6 col-md-offset-3">

  <div class="edit-lab-form">

    <h1>Редактирование лабораторной работы</h1>

    <%= simple_form_for([@subject, @lab]) do |f| %>
      <div id="rating-form">
        <label>Сложность</label>
      </div>
      <%= f.input :comment %>
      <%= f.button :submit, :class => "btn-custom2" %>
    <% end %>

  </div>
</div>

<script>
    $('#rating-form').raty({
        path: '/assets/',
        scoreName: 'lab[complexity]'
    });
</script>