<%= simple_form_for([@subject, @subject.labs.build]) do |f| %>
  <div id="rating-form">
    <label>Сложность</label>
  </div>
  <%= f.input :comment %>
  <span>Дата сдачи:</span>
  <!--<%= f.date_select :deadline %>-->
  <%= select_date Date.today, prefix: :deadline %>
  <%= f.button :submit, :class => "btn-custom2" %>
<% end %>

<script>
    $('#rating-form').raty({
        path: '/assets/',
        scoreName: 'lab[complexity]'
    });
</script>